{
  "config": {
    "default": true,
    "MD013": {
      "code_blocks": false,
      "tables": false,
      "headings": false,
      "strict": false,
      "stern": false
    },
    "MD033": {
      "allowed_elements": [
        "details",
        "summary",
        "sup",
        "sub",
        "kbd",
        "br",
        "hr",
        "img",
        "div",
        "span"
      ]
    },
    "MD041": false,
    "MD007": {
      "indent": 2
    }
  },
  "ignores": [
    "plugins/**/node_modules",
    "plugins/**/bower_components",
    "plugins/**/dist",
    "plugins/**/build"
  ]
}